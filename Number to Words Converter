#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int num, rev=0, zeros=0;
    scanf("%d", &num);
    if(num == 0)
        printf("Zero");
    else{
     while(num%10 == 0){
        zeros++;
        num = num/10;
    }
        while(num != 0){
            int rem = num%10;
            rev = rev * 10 + rem;
            num = num / 10;
        }       
    while(rev !=0){
        int digit = rev%10;
        switch(digit){
             case 0:
                  printf("Zero ");
                  break;
             case 1:
                    printf("One ");
                        break;
             case 2:
                     printf("Two ");
                        break;
             case 3:
                     printf("Three ");
                         break;
             case 4:
                     printf("Four ");
                         break;
             case 5:
                     printf("Five ");
                         break;
             case 6:
                     printf("Six ");
                         break;
             case 7:
                     printf("Sven ");
                         break;
             case 8:
                     printf("Eight ");
                         break;
             case 9:
                     printf("Nine ");
                         break;
                }
             rev = rev / 10;
        
            }
           for(int i=0; i< zeros; i++){
               printf("Zero ");
           }
       }
    return 0;

}
